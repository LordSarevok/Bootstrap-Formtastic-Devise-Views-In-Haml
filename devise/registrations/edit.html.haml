%h2 Editar mi cuenta
= semantic_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f|
  = f.semantic_errors
  = f.inputs do
    = f.input :email
    = f.input :password
    = f.input :password_confirmation
    = f.input :current_password
  = f.actions do
    = f.button 'Editar', class: "btn btn-success"
